{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "pnpm build",
  "outputDirectory": ".next",
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://alignment-ai.io/$1",
      "permanent": true
    }
  ]
}
